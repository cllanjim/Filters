<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_window_focused="false"
        android:drawable="@drawable/ic_launcher_round" />   <!--on-->
    <item android:state_checked="false" android:state_window_focused="false"
        android:drawable="@drawable/ic_setting" />      <!--off-->

    <item android:state_checked="true" android:state_pressed="true"
        android:drawable="@drawable/ic_launcher_round"/>    <!--on pressed-->
    <item android:state_checked="false" android:state_pressed="true"
        android:drawable="@drawable/ic_setting" />   <!--off pressed-->

    <item android:state_checked="true" android:state_focused="true"
        android:drawable="@drawable/ic_launcher_round" />       <!--on selected-->
    <item android:state_checked="false" android:state_focused="true"
        android:drawable="@drawable/ic_setting" />      <!--off selected-->

    <item android:state_checked="false" android:drawable="@drawable/ic_setting" />   <!--off-->
    <item android:state_checked="true" android:drawable="@drawable/ic_launcher_round" /> <!--on-->
</selector>